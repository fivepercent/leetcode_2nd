public class Solution {
    public int compareVersion(String version1, String version2) {
        int len1=version1.length();
        int len2=version2.length();
        int s1=0;
        int s2=0;
        int i1=0;
        int i2=0;
        while(i1<len1||i2<len2){
            while(i1<len1&&isNum(version1.charAt(i1)))i1++;
            while(i2<len2&&isNum(version2.charAt(i2)))i2++;
            int num1=s1<len1?Integer.valueOf(version1.substring(s1, i1)):0;
            int num2=s2<len2?Integer.valueOf(version2.substring(s2, i2)):0;
            System.out.println(num1+" vs "+num2);
            if(num1!=num2){
                return num1>num2?1:-1;
            }
            i1++;
            i2++;
            s1=i1;
            s2=i2;
        }
        return 0;
    }
    public boolean isNum(char c){
        return c<='9'&&c>='0';
    }
}
